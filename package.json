{
  "name": "preview-pro",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:ghpages": "vue-tsc --noEmit && vite build --base=/preview-pro/ --mode=ghpages",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.js,.vue src/ --fix",
    "lint:check": "eslint src/**/*.{ts,tsx,vue}",
    "prettier": "prettier -c --write 'src/**/*'"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.1.6",
